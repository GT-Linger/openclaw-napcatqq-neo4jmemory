[
  {
    "id": "qwen-main-sglang",
    "name": "Qwen 主智能体模型 (SGLang)",
    "modelPath": "qwen/Qwen2.5-7B-Instruct",
    "baseUrl": "http://192.168.1.100",
    "deploymentType": "command",
    "server": {
      "type": "remote",
      "host": "192.168.1.100",
      "port": 9000,
      "ssh": {
        "enabled": true,
        "host": "192.168.1.100",
        "username": "gpuuser",
        "privateKeyPath": "~/.ssh/id_rsa"
      }
    },
    "isMainAgent": true,
    "description": "主智能体专用模型 - 永久运行，不会被停止 (SGLang 后端)"
  },
  {
    "id": "deepseek-coder-sglang",
    "name": "DeepSeek Coder (SGLang 子智能体)",
    "modelPath": "deepseek-ai/DeepSeek-Coder-6.7B-Instruct",
    "deploymentType": "docker",
    "server": {
      "type": "docker",
      "host": "192.168.1.100",
      "port": 9001,
      "ssh": {
        "enabled": true,
        "host": "192.168.1.100",
        "username": "gpuuser",
        "privateKeyPath": "~/.ssh/id_rsa"
      },
      "docker": {
        "enabled": true,
        "image": "lmsysorg/sglang:latest",
        "gpuDevices": "0,1",
        "volumes": ["/models:/models"],
        "extraArgs": "--shm-size=16g"
      }
    },
    "isSubagentOnly": true,
    "description": "子智能体专用 - 任务完成后自动停止，使用 Docker 容器运行"
  },
  {
    "id": "qwen-docker-local-sglang",
    "name": "Qwen SGLang Docker 本地运行",
    "modelPath": "qwen/Qwen2.5-7B-Instruct",
    "deploymentType": "docker",
    "server": {
      "type": "docker",
      "host": "127.0.0.1",
      "docker": {
        "enabled": true,
        "image": "lmsysorg/sglang:latest",
        "gpuDevices": "all",
        "volumes": ["/path/to/models:/models"]
      }
    },
    "isSubagentOnly": true,
    "description": "本地 Docker 部署示例"
  }
]
